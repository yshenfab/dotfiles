defaults
tls on
auth on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile ~/.mutt/msmtp.log

# gmail
account yangfields
host smtp.gmail.com
port 587
from yangfields@gmail.com
user Yang
passwordeval "gpg --quiet --for-your-eyes-only --no-tty --decrypt ~/.mutt/gpg/yangfields@gmail.gpg"

account yshenfab
host smtp.gmail.com
port 587
from yshenfab@gmail.com
user Yang
passwordeval "gpg --quiet --for-your-eyes-only --no-tty --decrypt ~/.mutt/gpg/yshenfab@gmail.gpg"

# sustech
account sustech
host smtp.exmail.qq.com
port 587
#port 465
from sheny3@mail.sustech.edu.cn
user yshen
passwordeval "gpg --quiet --for-your-eyes-only --no-tty --decrypt ~/.mutt/gpg/sheny3@sustech.gpg"

